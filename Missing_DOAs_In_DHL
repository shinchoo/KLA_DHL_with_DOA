USE GEAR.INSIGHTS;


SELECT 
PRTNUMBER
FROM

(
SELECT *
FROM MFG_DOA_WITH_NC AS NC
WHERE YEAR("CREATE_DATE") > 2019
  AND YEAR("CREATE_DATE") < 2022
--  AND YEAR("CREATE_DATE") < 2022
ORDER BY "CREATE_DATE" ASC
)
--WHERE "PRTNUMBER" = '1498267'
LEFT JOIN SANDBOX.OPS.DHL_LPN_HISTORY_WIP AS DHL ON "PRTNUMBER" = DHL."PRID"
HAVING DHL."PRID" IS NULL
